{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "sequelize-cli": "^6.2.0",
    "start-server-and-test": "^1.11.6",
    "supertest": "^6.0.1"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit --runInBand",
    "start": "node server.js",
    "dev": "nodemon index.js",
    "drop": "node_modules/.bin/sequelize db:drop",
    "dropTest": "NODE_ENV=test node_modules/.bin/sequelize db:drop",
    "init": "node_modules/.bin/sequelize db:create",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "restart": "npm run drop && npm run init && npm run migrate && npm run seed",
    "restartTest": "NODE_ENV=test npm run restart",
    "restartAll": "npm run restart && npm run restartTest",
    "ciServer": "npm run init && npm run migrate && npm run test"
  },
